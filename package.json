{
  "name": "n8n-bookstack",
  "version": "1.0.0",
  "description": "Custom n8n node for BookStack API",
  "main": "dist/nodes/Bookstack/Bookstack.node.js",
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/BookstackApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Bookstack/Bookstack.node.js"
    ]
  },
  "scripts": {
    "build": "tsc && npm run copy-assets",
    "copy-assets": "mkdir -p dist/nodes/Bookstack && cp src/nodes/Bookstack/bookstack.svg dist/nodes/Bookstack/",
    "lint": "eslint . --ext .ts",
    "dev": "tsc --watch",
    "prepack": "npm run build"
  },
  "keywords": [
    "n8n",
    "bookstack",
    "connector",
    "api",
    "documentation",
    "wiki"
  ],
  "author": "Luca Guindani",
  "license": "MIT",
  "dependencies": {
    "n8n-core": "^1.0.0",
    "n8n-workflow": "^1.0.0"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/nock": "^10.0.3",
    "@types/node": "^20.0.0",
    "@types/ssh2": "^1.15.5",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.56.0",
    "typescript": "^5.2.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lucaguindani/n8n-bookstack.git"
  },
  "bugs": {
    "url": "https://github.com/lucaguindani/n8n-bookstack/issues"
  }
}
